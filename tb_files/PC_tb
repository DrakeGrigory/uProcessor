#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x643fcc527150 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x643fcc5272e0 .scope module, "PC_tb" "PC_tb" 3 10;
 .timescale -9 -12;
v0x643fcc53ce70_0 .var "AddrIn_tb", 5 0;
v0x643fcc53cf50_0 .net "AddrOut_tb", 5 0, v0x643fcc53ca50_0;  1 drivers
v0x643fcc53d020_0 .var "WriteEnable_tb", 0 0;
v0x643fcc53d120_0 .var "clk_tb", 0 0;
v0x643fcc53d1f0_0 .var "nReset_tb", 0 0;
S_0x643fcc4ec490 .scope module, "PC" "ProgramCounter" 3 18, 4 9 0, S_0x643fcc5272e0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "WriteEnable";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "nReset";
    .port_info 3 /INPUT 6 "AddrIn";
    .port_info 4 /OUTPUT 6 "AddrOut";
v0x643fcc4ec690_0 .net "AddrIn", 5 0, v0x643fcc53ce70_0;  1 drivers
v0x643fcc53ca50_0 .var "AddrOut", 5 0;
v0x643fcc53cb30_0 .net "WriteEnable", 0 0, v0x643fcc53d020_0;  1 drivers
v0x643fcc53cc00_0 .net "clk", 0 0, v0x643fcc53d120_0;  1 drivers
v0x643fcc53ccc0_0 .net "nReset", 0 0, v0x643fcc53d1f0_0;  1 drivers
E_0x643fcc527f30 .event posedge, v0x643fcc53cc00_0;
    .scope S_0x643fcc4ec490;
T_0 ;
    %wait E_0x643fcc527f30;
    %load/vec4 v0x643fcc53ccc0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/1 T_0.2, 4;
    %flag_mov 8, 4;
    %load/vec4 v0x643fcc53cb30_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %flag_get/vec4 4;
    %jmp/0 T_0.3, 4;
    %load/vec4 v0x643fcc4ec690_0;
    %pushi/vec4 0, 0, 6;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_0.3;
    %flag_set/vec4 9;
    %flag_or 9, 8;
    %flag_mov 4, 9;
T_0.2;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 6;
    %store/vec4 v0x643fcc53ca50_0, 0, 6;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x643fcc53cb30_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x643fcc4ec690_0;
    %store/vec4 v0x643fcc53ca50_0, 0, 6;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x643fcc53ca50_0;
    %addi 1, 0, 6;
    %store/vec4 v0x643fcc53ca50_0, 0, 6;
T_0.5 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x643fcc5272e0;
T_1 ;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x643fcc53d120_0;
    %inv;
    %store/vec4 v0x643fcc53d120_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x643fcc5272e0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x643fcc53d120_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x643fcc53d1f0_0, 0, 1;
    %delay 6000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x643fcc53d1f0_0, 0, 1;
    %delay 250000, 0;
    %vpi_call/w 3 38 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x643fcc5272e0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x643fcc53d020_0, 0, 1;
    %pushi/vec4 22, 0, 6;
    %store/vec4 v0x643fcc53ce70_0, 0, 6;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x643fcc53d020_0, 0, 1;
    %pushi/vec4 0, 0, 6;
    %store/vec4 v0x643fcc53ce70_0, 0, 6;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x643fcc53d020_0, 0, 1;
    %pushi/vec4 55, 0, 6;
    %store/vec4 v0x643fcc53ce70_0, 0, 6;
    %delay 30000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x643fcc53d020_0, 0, 1;
    %pushi/vec4 26, 0, 6;
    %store/vec4 v0x643fcc53ce70_0, 0, 6;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x643fcc53d020_0, 0, 1;
    %pushi/vec4 55, 0, 6;
    %store/vec4 v0x643fcc53ce70_0, 0, 6;
    %end;
    .thread T_3;
    .scope S_0x643fcc5272e0;
T_4 ;
    %vpi_call/w 3 66 "$dumpfile", "tb_files/PC_tb.vcd" {0 0 0};
    %vpi_call/w 3 67 "$dumpvars" {0 0 0};
    %vpi_call/w 3 68 "$dumpon" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
